              "importance": {
                  "viewClass": "SelectView",
                  "prompt": "Select Event Importance",
                  "label": "Importance",
                  "required": false,
                  "model": {
                      "Low": "Low",
                      "Normal": "Normal",
                      "High": "High"
                  }
              },
              "showAs": {
                  "viewClass": "SelectView",
                  "prompt": "Select Show As value",
                  "label": "Show As",
                  "required": false,
                  "model": {
                      "Free": "Free",
                      "Tentative": "Tentative",
                      "Busy": "Busy",
                      "Oof": "Out of Office",
                      "WorkingElsewhere": "Working Elsewhere",
                      "Unknown": "Unknown"
                  }
              },
              "sensitivity": {
                  "viewClass": "SelectView",
                  "prompt": "Select Event Sensitivity",
                  "label": "Sensitivity",
                  "required": false,
                  "model": {
                      "Normal": "Normal",
                      "Personal": "Personal",
                      "Private": "Private",
                      "Confidential": "Confidential"
                  }
              },
              "bodyContentType": {
                  "viewClass": "SelectView",
                  "prompt": "Select Event Body Content Type",
                  "label": "Body Content Type",
